{
  "name": "hedgemate",
  "private": true,
  "version": "1.0.0",
  "description": "Monorepo for HedgeMate Health Guardian backend + frontend",
  "homepage": "https://github.com/ksacheth/HedgeMate",
  "engines": {
    "node": "^22.16.0",
    "pnpm": "10.7.0"
  },
  "type": "module",
  "scripts": {
    "build": "pnpm -r run build",
    "lint": "pnpm -r lint",
    "clean": "pnpm exec rm -Rf node_modules dist && pnpm --parallel -r run clean",
    "test": "pnpm -r run test",
    "dev": "pnpm build && pnpm -r run dev",
    "prepare": "husky",
    "start": "pnpm -r run start"
  },
  "packageManager": "pnpm@10.7.0",
  "keywords": [],
  "pnpm": {
    "overrides": {
      "ethers": "^5.8.0",
      "zod": "^4.1.12"
    },
    "peerDependencyRules": {
      "allowedVersions": {
        "ethers": "5.8.0",
        "zod": "4.1.12"
      }
    }
  },
  "devDependencies": {
    "@dotenvx/dotenvx": "^0.26.0",
    "@tsconfig/node20": "^20.1.5",
    "@types/node": "^20.11.30",
    "@types/verror": "^1.10.10",
    "@typescript-eslint/eslint-plugin": "^7.3.1",
    "@typescript-eslint/parser": "^7.3.1",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-import-resolver-typescript": "^4.2.3",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-sort-destructure-keys": "^1.5.0",
    "eslint-plugin-sort-keys-plus": "^1.4.0",
    "eslint-plugin-typescript-sort-keys": "^3.2.0",
    "husky": "^9.0.11",
    "lint-staged": "^15.2.2",
    "prettier": "^3.2.5",
    "prettier-plugin-jsdoc": "^1.3.0",
    "react-circular-progressbar": "^2.2.0",
    "tsx": "^4.7.1",
    "typescript": "^5.4.3",
    "typescript-eslint": "^8.29.0"
  },
  "dependencies": {
    "consola": "^3.2.3",
    "tslib": "^2.8.1"
  }
}
